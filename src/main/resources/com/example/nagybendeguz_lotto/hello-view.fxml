<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.example.nagybendeguz_lotto.HelloController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
    <rowConstraints>
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" />
        <RowConstraints maxHeight="30.0" minHeight="30.0" prefHeight="30.0" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
        <ColumnConstraints maxWidth="100.0" minWidth="100.0" prefWidth="100.0" />
    </columnConstraints>
    <children>
        <Button fx:id="btnSorsolRendez" maxWidth="100.0" minWidth="100.0" mnemonicParsing="false"
                onAction="#sorsolKatt" prefWidth="100.0" stylesheets="@style.css" text="Sorsol" />
        <Label fx:id="nagySzam" alignment="CENTER" textAlignment="CENTER" maxHeight="30.0" maxWidth="100.0"
               minHeight="30.0" minWidth="100.0" prefHeight="30.0" prefWidth="100.0" stylesheets="@style.css"
               GridPane.columnIndex="1" />
        <Label fx:id="kiirtSzamok" prefWidth="200.0" stylesheets="@style.css" textAlignment="CENTER"
               GridPane.columnSpan="2" GridPane.rowIndex="1" />
    </children>
</GridPane>
